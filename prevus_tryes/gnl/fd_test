	test_line = ft_strdup("ceci est une phrase test \n bails cachee \n c'etais une trap");
	line = ft_strdup("");
	memory->str = ft_strdup(test_line);
	memory->str_size = strlen(test_line);
	check_str(memory);
	f_print("line before = ", line, 1);
	join(&line, memory);
	f_print("line after  = ", line, 2);
	f_print("    memory  = ", memory->str, 3);

//	THIRD JOIN TEST ||			LIGNE VIDE + STR\N

	test_line = ft_strdup("ceci est une phrase test");
	line = ft_strdup("");
	memory->str = ft_strdup(test_line);
	memory->str_size = strlen(test_line);
	check_str(memory);
	f_print("line before = ", line, 1);
	join(&line, memory);
	f_print("line after  = ", line, 2);
	f_print("    memory  = ", memory->str, 3);

//	SECOND JOIN TEST ||			LIGNE NON-VIDE + STR \N

	test_line = ft_strdup("ceci est une phrase test \n bails cachee \n c'etais une trap");
	line = ft_strdup("Salut ceci est une ligne non vide ");
	memory->str = ft_strdup(test_line);
	memory->str_size = strlen(test_line);
	check_str(memory);
	f_print("line before = ", line, 1);
	join(&line, memory);
	f_print("line after  = ", line, 2);
	f_print("    memory  = ", memory->str, 4);

//	LAST TEST ||				LIGNE NON-VIDE + STR FULL

	test_line = ft_strdup("ceci est une phrase test");
	line = ft_strdup("Salut ceci est une ligne non vide ");
	memory->str = ft_strdup(test_line);
	memory->str_size = strlen(test_line);
	check_str(memory);
	f_print("line before = ", line, 1);
	join(&line, memory);
	f_print("line after  = ", line, 2);
	f_print("    memory  = ", memory->str, 4);

